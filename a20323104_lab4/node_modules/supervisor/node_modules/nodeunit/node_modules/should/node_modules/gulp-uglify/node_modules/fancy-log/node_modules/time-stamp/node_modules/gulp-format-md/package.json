{
  "name": "gulp-format-md",
  "description": "Gulp plugin for beautifying markdown using pretty-remarkable.",
  "version": "0.1.7",
  "homepage": "https://github.com/jonschlinkert/gulp-format-md",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gulp-format-md",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gulp-format-md/issues"
  },
  "license": "MIT",
  "files": [
    "lib/",
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "get-value": "^2.0.3",
    "lazy-cache": "^1.0.3",
    "plugin-error": "^0.1.2",
    "pretty-remarkable": "^0.2.0",
    "remarkable": "^1.6.2",
    "sections": "^0.1.1",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.1.1",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "mocha": "*"
  },
  "keywords": [
    "beautify",
    "format",
    "formatter",
    "gulp",
    "gulpplugin",
    "markdown",
    "md",
    "prettify",
    "pretty",
    "remarkable"
  ],
  "verb": {
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": {
      "./": {}
    },
    "related": {
      "list": [
        "gulp",
        "pretty-remarkable",
        "remarkable"
      ]
    }
  }
}
